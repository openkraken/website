{
  "private": true,
  "name": "dumi-app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openkraken/website.git",
    "branch": "main",
    "platform": "github"
  },
  "scripts": {
    "start": "dumi dev",
    "build": "dumi build",
    "translateAll": "node translateAll.js",
    "translate": "node translateFile.js",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "deploy": "gh-pages -d dist"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "dumi": "^1.1.11",
    "dumi-theme-default": "~1.0.0",
    "gh-pages": "^3.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "prism-react-renderer": "^1.2.0",
    "slash": "^3.0.0",
    "sleep": "^6.3.0"
  }
}
